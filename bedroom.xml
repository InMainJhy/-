      <geom name="floor_lamp_base" type="cylinder" size="0.2 0.05" pos="0 0 0.05" material="metal"/>
      <geom name="floor_lamp_pole" type="cylinder" size="0.03 1.5" pos="0 0 0.8" material="metal"/>
      <geom name="floor_lamp_shade" type="cylinder" size="0.25 0.25" pos="0 0 1.9" material="light" euler="90 0 0"/>
    </body>
    
    <!-- 天花板吊灯 -->
    <body name="ceiling_light" pos="0 0 2.5">
      <geom name="ceiling_light_base" type="cylinder" size="0.1 0.05" pos="0 0 -0.05" material="metal"/>
      <geom name="ceiling_light_chain" type="cylinder" size="0.01 0.2" pos="0 0 -0.2" material="metal"/>
      <geom name="ceiling_light_shade" type="cylinder" size="0.2 0.15" pos="0 0 -0.4" material="light" euler="90 0 0"/>
    </body>
    
    <!-- 衣柜1：左侧墙壁 -->
    <body name="wardrobe1" pos="-4 0 1">
      <geom name="wardrobe1_body" type="box" size="0.5 0.6 1.5" pos="0 0 1" material="wood"/>
      <geom name="wardrobe1_door1" type="box" size="0.45 0.55 0.02" pos="-0.25 0 1" material="wardrobe_door"/>
      <geom name="wardrobe1_door2" type="box" size="0.45 0.55 0.02" pos="0.25 0 1" material="wardrobe_door"/>
      <geom name="wardrobe1_shelf1" type="box" size="0.4 0.5 0.02" pos="0 0 1.5" material="wardrobe_inner"/>
      <geom name="wardrobe1_shelf2" type="box" size="0.4 0.5 0.02" pos="0 0 0.8" material="wardrobe_inner"/>
    </body>
    
    <!-- 衣柜2：右侧墙壁 -->
    <body name="wardrobe2" pos="4 0 1">
      <geom name="wardrobe2_body" type="box" size="0.5 0.6 1.5" pos="0 0 1" material="wood"/>
      <geom name="wardrobe2_door1" type="box" size="0.45 0.55 0.02" pos="-0.25 0 1" material="wardrobe_door"/>
      <geom name="wardrobe2_door2" type="box" size="0.45 0.55 0.02" pos="0.25 0 1" material="wardrobe_door"/>
      <geom name="wardrobe2_shelf1" type="box" size="0.4 0.5 0.02" pos="0 0 1.5" material="wardrobe_inner"/>
      <geom name="wardrobe2_shelf2" type="box" size="0.4 0.5 0.02" pos="0 0 0.8" material="wardrobe_inner"/>
    </body>
    
    <!-- 新桌子：中央偏前 -->
    <body name="dining_table" pos="0 2 0">
      <geom name="dining_table_top" type="cylinder" size="0.8 0.05" pos="0 0 0.6" material="wood"/>
      <geom name="dining_table_leg1" type="cylinder" size="0.05 0.3" pos="-0.5 0.5 0.3" material="wood"/>
      <geom name="dining_table_leg2" type="cylinder" size="0.05 0.3" pos="0.5 0.5 0.3" material="wood"/>
      <geom name="dining_table_leg3" type="cylinder" size="0.05 0.3" pos="-0.5 -0.5 0.3" material="wood"/>
      <geom name="dining_table_leg4" type="cylinder" size="0.05 0.3" pos="0.5 -0.5 0.3" material="wood"/>
      
      <!-- 台灯 -->
      <body name="dining_table_lamp" pos="0.5 0 0.6">
        <geom name="dining_lamp_base" type="cylinder" size="0.1 0.05" pos="0 0 0.05" material="metal"/>
        <geom name="dining_lamp_pole" type="cylinder" size="0.02 0.3" pos="0 0 0.25" material="metal"/>
        <geom name="dining_lamp_shade" type="cylinder" size="0.12 0.12" pos="0 0 0.45" material="light" euler="90 0 0"/>
      </body>
      
      <!-- 水果 -->
      <!-- 苹果 -->
      <geom name="apple1" type="sphere" size="0.08" pos="-0.3 0.2 0.68" material="apple"/>
      <geom name="apple2" type="sphere" size="0.08" pos="-0.1 0.25 0.68" material="apple"/>
      <geom name="apple3" type="sphere" size="0.08" pos="0.1 0.2 0.68" material="apple"/>
      
      <!-- 香蕉 -->
      <geom name="banana1" type="capsule" size="0.06 0.12" pos="-0.2 -0.2 0.68" material="banana" euler="0 30 0"/>
      <geom name="banana2" type="capsule" size="0.06 0.12" pos="0 -0.2 0.68" material="banana" euler="0 0 0"/>
      <geom name="banana3" type="capsule" size="0.06 0.12" pos="0.2 -0.2 0.68" material="banana" euler="0 -30 0"/>
      
      <!-- 橙子 -->
      <geom name="orange1" type="sphere" size="0.09" pos="0.3 -0.1 0.68" material="orange"/>
      <geom name="orange2" type="sphere" size="0.09" pos="0.4 0.1 0.68" material="orange"/>
      
      <!-- 葡萄 -->
      <geom name="grape1" type="sphere" size="0.04" pos="-0.35 -0.1 0.68" material="grape"/>
      <geom name="grape2" type="sphere" size="0.04" pos="-0.3 -0.05 0.72" material="grape"/>
      <geom name="grape3" type="sphere" size="0.04" pos="-0.25 -0.1 0.68" material="grape"/>
      <geom name="grape4" type="sphere" size="0.04" pos="-0.3 -0.15 0.72" material="grape"/>
    </body>
    
    <!-- 机器人 -->
    <body name="robot_base" pos="0 0 0">
      <!-- 移动关节 -->
      <joint name="robot_x_joint" type="slide" axis="1 0 0"/>
      <joint name="robot_y_joint" type="slide" axis="0 1 0"/>
      <joint name="robot_rot_joint" type="hinge" axis="0 0 1"/>
      
      <!-- 机器人主体 -->
      <body name="robot" pos="0 0 0.5">
        <!-- 底盘 -->
        <geom name="chassis" type="box" size="0.4 0.4 0.2" pos="0 0 0" material="metal"/>
        
        <!-- 球形轮子 -->
        <geom name="wheel_front_left" type="sphere" size="0.15" pos="-0.3 -0.3 -0.2" material="metal"/>
        <geom name="wheel_front_right" type="sphere" size="0.15" pos="0.3 -0.3 -0.2" material="metal"/>
        <geom name="wheel_back_left" type="sphere" size="0.15" pos="-0.3 0.3 -0.2" material="metal"/>
        <geom name="wheel_back_right" type="sphere" size="0.15" pos="0.3 0.3 -0.2" material="metal"/>
      
        <!-- 头部 -->
        <body name="head" pos="0 0 0.4">
          <geom name="head_geom" type="sphere" size="0.15" material="plastic"/>
        </body>
      
        <!-- 左臂 -->
        <body name="left_shoulder" pos="-0.4 0 0.2">
          <joint name="left_shoulder_joint" type="ball"/>
          <geom name="left_shoulder_geom" type="cylinder" size="0.08 0.1" pos="-0.1 0 0" material="metal" euler="0 90 0"/>
          
          <body name="left_elbow" pos="-0.2 0 0">
            <joint name="left_elbow_joint" type="hinge" axis="0 1 0" range="-90 90"/>
            <geom name="left_upper_arm" type="cylinder" size="0.05 0.2" pos="-0.2 0 0" material="metal" euler="0 90 0"/>
            
            <body name="left_wrist" pos="-0.4 0 0">
              <joint name="left_wrist_joint" type="hinge" axis="0 1 0" range="-90 90"/>
              <geom name="left_lower_arm" type="cylinder" size="0.03 0.15" pos="-0.15 0 0" material="metal" euler="0 90 0"/>
              <geom name="left_hand" type="sphere" size="0.05" pos="-0.3 0 0" material="plastic"/>
            </body>
          </body>
        </body>
      
        <!-- 右臂 -->
        <body name="right_shoulder" pos="0.4 0 0.2">
          <joint name="right_shoulder_joint" type="ball"/>
          <geom name="right_shoulder_geom" type="cylinder" size="0.08 0.1" pos="0.1 0 0" material="metal" euler="0 90 0"/>
          
          <body name="right_elbow" pos="0.2 0 0">
            <joint name="right_elbow_joint" type="hinge" axis="0 1 0" range="-90 90"/>
            <geom name="right_upper_arm" type="cylinder" size="0.05 0.2" pos="0.2 0 0" material="metal" euler="0 90 0"/>
            
            <body name="right_wrist" pos="0.4 0 0">
              <joint name="right_wrist_joint" type="hinge" axis="0 1 0" range="-90 90"/>
              <geom name="right_lower_arm" type="cylinder" size="0.03 0.15" pos="0.15 0 0" material="metal" euler="0 90 0"/>
              <geom name="right_hand" type="sphere" size="0.05" pos="0.3 0 0" material="plastic"/>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>
  
  <actuator>
    <!-- 机器人手臂关节控制器 -->
    <motor name="left_shoulder_motor" joint="left_shoulder_joint" gear="5"/>
    <motor name="left_elbow_motor" joint="left_elbow_joint" gear="5"/>
    <motor name="left_wrist_motor" joint="left_wrist_joint" gear="5"/>
    
    <motor name="right_shoulder_motor" joint="right_shoulder_joint" gear="5"/>
    <motor name="right_elbow_motor" joint="right_elbow_joint" gear="5"/>
    <motor name="right_wrist_motor" joint="right_wrist_joint" gear="5"/>
    
    <!-- 机器人行走控制器 -->
    <motor name="robot_x_motor" joint="robot_x_joint" gear="100" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="robot_y_motor" joint="robot_y_joint" gear="100" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="robot_rot_motor" joint="robot_rot_joint" gear="100" ctrllimited="true" ctrlrange="-1 1"/>
    
    <!-- 门控制器 -->
    <motor name="door_motor" joint="door_hinge" gear="50" ctrllimited="true" ctrlrange="-1 1"/>
  </actuator>
</mujoco>