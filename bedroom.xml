    <!-- 光照设置 -->
    <!-- 方向光（模拟太阳光） -->
    <light diffuse="1 1 1" pos="5 -5 5" dir="-1 1 -1"/>
    <!-- 点光源1（天花板灯） -->
    <light diffuse="1 1 0.9" pos="0 0 2.3"/>
    <!-- 点光源2（落地灯） -->
    <light diffuse="1 1 0.9" pos="3 2 1.9"/>
    <!-- 点光源3（小桌子台灯） -->
    <light diffuse="1 1 0.9" pos="-3 1 1.05"/>
    <!-- 点光源4（餐桌台灯） -->
    <light diffuse="1 1 0.9" pos="0.5 2 1.05"/>
    
